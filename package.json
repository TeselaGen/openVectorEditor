{
  "name": "open-vector-editor",
  "version": "18.3.4",
  "description": "Teselagen's Open Source Vector Editor",
  "main": "lib/index.js",
  "module": "es/index.js",
  "exports": {
    "import": "./es/index.js",
    "require": "./lib/index.js"
  },
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "VITE_start": "vite --port 3344",
    "VITE_build": "vite build",
    "VITE_preview": "vite preview",
    "VITE_buildWithDemo": "vite build -c vite.config.demo.js",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build": "nwb build-react-component --copy-files --no-demo",
    "start": "nwb serve-react-demo --port 3344",
    "localj5": "AMQP=localhost:5672 REMOTE_J5=http://localhost:10000 nwb serve-react-demo --port 3344",
    "c": "yarn cypress open --e2e --browser chrome",
    "test-e2e": "yarn cypress run --record --key 373658d4-4931-4ffd-9263-8eb78a838ab9",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@blueprintjs/core": "3.52.0",
    "@blueprintjs/datetime": "3.23.19",
    "@blueprintjs/select": "3.18.11",
    "@fontsource/ubuntu-mono": "^4.5.11",
    "@hello-pangea/dnd": "^16.2.0",
    "@risingstack/react-easy-state": "^6.3.0",
    "@teselagen/bio-parsers": "^0.1.13",
    "@teselagen/range-utils": "^0.1.12",
    "@teselagen/react-list": "^0.8.18",
    "@teselagen/sequence-utils": "^0.1.12",
    "@teselagen/ui": "^0.0.2",
    "@use-gesture/react": "^10.2.23",
    "classnames": "^2.3.2",
    "clipboard": "2.0.8",
    "color": "^3.2.1",
    "combokeys": "^3.0.1",
    "deep-equal": "^1.1.1",
    "dom-to-image": "^2.6.0",
    "downloadjs": "^1.4.7",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "immer": "^9.0.19",
    "is-mobile": "^3.1.1",
    "lodash": "^4.17.21",
    "node-interval-tree": "^1.3.3",
    "paths-js": "^0.4.11",
    "pluralize": "^7.0.0",
    "popper.js": "^1.16.1",
    "prop-types": "^15.8.1",
    "react-draggable": "4.4.4",
    "react-dropzone": "^11.7.1",
    "react-measure": "^2.5.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^4.3.1",
    "react-sizeme": "^2.6.12",
    "recompose": "npm:react-recompose@^0.31.1",
    "redux": "^4.2.0",
    "redux-act": "^1.8.0",
    "redux-form": "^8.3.10",
    "redux-ignore": "^1.2.5",
    "redux-thunk": "2.4.1",
    "reselect": "^4.1.7",
    "shortid": "^2.2.16",
    "teselagen-react-components": "30.14.30",
    "tg-use-local-storage-state": "^16.0.3",
    "use-debounce": "^8.0.4",
    "use-local-storage-state": "^13.0.0",
    "validate.io-nonnegative-integer-array": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.9.0"
  },
  "optionalDependencies": {
    "cypress": "12.16.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@vitejs/plugin-react": "^4.0.0",
    "auto-changelog": "^2.4.0",
    "babel-plugin-istanbul": "^6.1.1",
    "copy-to-clipboard": "^3.3.3",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.35.0",
    "eslint-config-teselagen": "6.0.17",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "10.5.4",
    "nodemon": "^2.0.21",
    "nwb": "0.25.2",
    "papaparse": "^5.4.0",
    "prettier": "2.8.4",
    "react": "^17.0.2",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^17.0.2",
    "react-markdown": "8.0.7",
    "to-regex-range": "^5.0.1",
    "vite": "^4.3.9"
  },
  "resolutions": {
    "listr": "0.14.3"
  },
  "lint-staged": {
    "addons/**/*.js|src/**/*.js|demo/**/*.js|cypress/**/*.js|nwb.config.js": [
      "eslint --rule 'no-ignore: 0' --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write"
    ],
    "**/*.css": [
      "prettier --write"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/openVectorEditor",
  "homepage": "https://github.com/TeselaGen/openVectorEditor",
  "keywords": [
    "react-component",
    "plasmid",
    "vector",
    "editor",
    "alignment",
    "sequence",
    "sequence-alignment",
    "msa",
    "multiple-sequence-alignment",
    "pairwise",
    "pairwise-alignment",
    "teselagen"
  ]
}
