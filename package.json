{
  "name": "open-vector-editor",
  "version": "4.0.13",
  "description": "Teselagen's Open Source Vector Editor",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3344",
    "precommit": "lint-staged",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "test": "jest --env=jsdom --maxWorkers 2",
    "test-coverage": "yarn test -- --coverage",
    "test:staged": "cross-env CI=true yarn test -- --findRelatedTests",
    "test-fix": "git reset . && cross-env CI=true yarn test --onlyChanged --updateSnapshot",
    "test-watch": "yarn test --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "clipboard": "^2.0.1",
    "color": "^3.0.0",
    "combokeys": "^3.0.0",
    "deep-equal": "^1.0.1",
    "file-saver": "^1.3.8",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.10",
    "lru-memoize": "^1.0.2",
    "node-interval-tree": "^1.3.3",
    "paths-js": "^0.4.5",
    "randomcolor": "^0.5.3",
    "react-beautiful-dnd": "^4.0.1",
    "react-draggable": "^3.0.3",
    "react-dropzone": "^4.2.10",
    "react-easy-state": "^5.0.1",
    "react-list": "^0.8.8",
    "react-sizeme": "^2.4.4",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-act": "^1.7.4",
    "redux-ignore": "^1.2.5",
    "redux-undo": "1.0.0-beta9-9-7",
    "reselect": "^3.0.1",
    "tether": "^1.4.1",
    "tg-react-reflex": "^2.0.10",
    "uniqid": "^4.1.1"
  },
  "peerDependencies": {
    "apollo-cache-invalidation": "^0.0.3",
    "bio-parsers": "^4.2.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.6.0",
    "teselagen-react-components": "^22.0.0",
    "tg-client-query-builder": "^2.12.1",
    "ve-range-utils": "^2.3.0",
    "ve-sequence-utils": "^3.2.11"
  },
  "devDependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@blueprintjs/datetime": "^2.0.3",
    "apollo-cache-invalidation": "^0.0.3",
    "apollo-client": "^2.3.2",
    "axios": "^0.18.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bio-parsers": "4.4.10",
    "chance": "^1.0.16",
    "cross-env": "^5.1.6",
    "enzyme": "^3.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-teselagen": "^4.0.12",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "install-peerdeps": "^1.6.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.2",
    "moment": "2.22.1",
    "nodemon": "^1.17.5",
    "nwb": "^0.21.5",
    "prettier": "^1.13.3",
    "react": "^16.4.0",
    "react-addons-perf": "^15.4.2",
    "react-apollo": "^2.1.4",
    "react-collapsible": "^2.2.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10",
    "react-table": "^6.8.6",
    "react-transition-group": "^2.3.1",
    "redux-form": "^7.3.0",
    "redux-thunk": "2.3.0",
    "teselagen-react-components": "^22.1.26",
    "tg-client-query-builder": "^2.12.1",
    "ve-range-utils": "^2.5.0",
    "ve-sequence-utils": "3.2.56"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/openVectorEditor",
  "keywords": [
    "react-component",
    "plasmid",
    "vector",
    "editor",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
