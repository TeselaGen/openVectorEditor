{
  "name": "open-vector-editor",
  "version": "10.1.5",
  "description": "Teselagen's Open Source Vector Editor",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon  --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "//sl": "webpack-link && source .linkEnv; nwb serve-react-demo --port 3344",
    "start": "nwb serve-react-demo --port 3344",
    "localj5": "AMQP=localhost:5672 REMOTE_J5=http://localhost:10000 nwb serve-react-demo --port 3344",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "postpublish": "yarn deploy",
    "predeploy": "yarn buildWithDemo",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d demo/dist",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "c": "yarn cypress open",
    "test-e2e": "yarn cypress run --record --key 0dcf0c08-68ce-4547-aaaa-ac33af76e4a9"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "color": "^3.1.2",
    "combokeys": "^3.0.1",
    "deep-equal": "^1.0.1",
    "file-saver": "^1.3.8",
    "inferno-compat": "^7.2.1",
    "json.sortify": "^2.2.2",
    "node-interval-tree": "^1.3.3",
    "paths-js": "^0.4.5",
    "pluralize": "^7.0.0",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react-beautiful-dnd": "^10.1.0",
    "react-draggable": "^3.2.1",
    "react-dropzone": "^4.2.11",
    "react-easy-state": "^5.0.1",
    "react-list": "^0.8.11",
    "react-measure": "^2.2.4",
    "react-sizeme": "^2.5.2",
    "react-to-print": "^2.0.0-alpha-4",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-act": "^1.7.7",
    "redux-ignore": "^1.2.5",
    "reselect": "^3.0.1",
    "tg-react-reflex": "^2.0.10",
    "typeface-ubuntu-mono": "^0.0.54",
    "uniqid": "^4.1.1"
  },
  "peerDependencies": {
    "apollo-cache-invalidation": "^0.0.3",
    "bio-parsers": "^4.2.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.6.0",
    "teselagen-react-components": "^25.0.9",
    "tg-client-query-builder": "^2.12.1",
    "ve-range-utils": "^2.3.0",
    "ve-sequence-utils": "^3.2.11"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.18.0",
    "@blueprintjs/datetime": "^3.9.2",
    "@blueprintjs/select": "^3.10.0",
    "@teselagen/apollo-methods": "^1.2.1",
    "@types/lodash": "npm:@types/lodash-es",
    "apollo-cache-invalidation": "^0.0.3",
    "apollo-client": "2.5.1",
    "axios": "^0.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bio-parsers": "5.6.1",
    "chance": "^1.0.18",
    "cross-env": "^5.1.6",
    "cypress": "^3.4.1",
    "drag-mock": "^1.4.0",
    "eslint": "^6.2.1",
    "eslint-config-teselagen": "^5.0.5",
    "gh-pages": "^2.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "inquirer": "^6.2.2",
    "install-peerdeps": "1.10.2",
    "lint-staged": "9.2.3",
    "moment": "2.24.0",
    "nwb": "0.23.0",
    "prettier": "1.16.4",
    "react": "16.8.6",
    "react-addons-perf": "^15.4.2",
    "react-apollo": "^2.5.8",
    "react-dom": "16.8.6",
    "react-markdown": "^4.0.6",
    "react-redux": "5",
    "react-router-dom": "4.3.1",
    "react-select": "^3.0.4",
    "react-table": "^6.9.2",
    "react-transition-group": "^2.6.0",
    "redux-form": "7",
    "redux-thunk": "2.3.0",
    "teselagen-react-components": "^27.1.21",
    "tg-client-query-builder": "^2.12.2",
    "ve-range-utils": "^2.5.5",
    "ve-sequence-utils": "^3.3.46",
    "why-did-you-update": "^1.0.6"
  },
  "resolutions": {
    "listr": "0.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js|demo/**/*.js|cypress/**/*.js": [
      "eslint --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/openVectorEditor",
  "keywords": [
    "react-component",
    "plasmid",
    "vector",
    "editor",
    "teselagen"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
